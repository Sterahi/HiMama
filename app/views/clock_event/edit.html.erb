<div class ="row">
    <%= form_for @event, url: 'update' do |f| %>
        <label for = "event_type">Type of Event</label>
        <%= f.text_field :event_type, class: 'form-control' %>
        <br />
        <label for = "user_id">User</label>

        <input class ="form-control" disabled value = <%= @event.user.first.name %> />
        <br />
        <%= f.submit class: 'btn btn-primary'%>
    <% end %>
</div>
